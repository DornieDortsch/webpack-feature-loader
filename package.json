{
  "name": "webpack-contextreplacement-bug",
  "version": "1.0.0",
  "description": "Minimal sample project to report webpack bug with ContextReplacementPlugin while using dev-server",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "webpack --config webpack.config.js",
    "serve": "serve ./dist",
    "dev": "webpack-dev-server",
    "pretest": "npm run build",
    "test": "jest",
    "test:unit": "npm run build && jest \"src/.*\\.test\\.js$\"",
    "test:integration": "npm run build && jest \"src/.*\\.it\\.js$\"",
    "tdd": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DornieDortsch/webpack-contextreplacement-bug.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Donie Dortsch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DornieDortsch/webpack-contextreplacement-bug/issues"
  },
  "homepage": "https://github.com/DornieDortsch/webpack-contextreplacement-bug#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-puppeteer": "^2.4.0",
    "puppeteer": "^1.3.0",
    "serve": "6.5.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "zepto-webpack": "^1.2.1"
  }
}
