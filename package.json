{
  "name": "webpack-feature-loader-example",
  "version": "1.0.0",
  "description": "Sample project to declarative load js features (modules) with webpack.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && serve ./dist",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server  --open --config webpack.dev.js",
    "tdd": "jest --watchAll",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest \".*\\.test\\.js$\"",
    "test:integration": "jest \".*\\.it\\.js$\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DornieDortsch/webpack-feature-loader.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Donie Dortsch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DornieDortsch/webpack-feature-loader.git/issues"
  },
  "homepage": "https://github.com/DornieDortsch/webpack-feature-loader.git#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-puppeteer": "^2.4.0",
    "puppeteer": "^1.3.0",
    "serve": "6.5.8",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "zepto-webpack": "^1.2.1"
  }
}
